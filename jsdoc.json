{
  "source": {
    "include": ["src/components"],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.(js|ts|vue)$",
    "excludePattern": "(node_modules|docs)"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "verbose": true
  },
  "plugins": ["node_modules/jsdoc-vuejs"]
}
